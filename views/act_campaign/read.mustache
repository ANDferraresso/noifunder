{{> header }}

<div class="container-fluid">
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item"><a href="{{ cr.links.home }}">Home</a></li>
      <li class="breadcrumb-item">{{ cr.T.tit_Activities }}</li>
      <li class="breadcrumb-item">{{ cr.T.tit_Campaigns }}</li>
      <li class="breadcrumb-item"><a href="{{ cr.links.act_campaign_list }}/1">{{ cr.T.tit_ListCampaigns }}</a></li>
      <li class="breadcrumb-item active">{{ cr.T.tit_View }}</li>
    </ol>
  </nav>
</div><!-- /container -->

<main role="main">
<div class="container-fluid">
<h1>{{ cr.T.tit_ViewCampaign }}</h1>

{{{ lc.act_campaignTab.rendered }}}

<div class="btn-group btn-group-sm" role="group">
  <a class="btn btn-secondary" href="{{{ cr.links.act_campaign_update }}}/{{ lc.act_campaignTab.data.0.rid }}" role="button">{{{ cr.T.btn_Update }}}</a>
  <a class="btn btn-secondary" href="{{{ cr.links.act_campaign_delete }}}/{{ lc.act_campaignTab.data.0.rid }}" role="button">{{{ cr.T.btn_Delete }}}</a>
  <a class="btn btn-secondary" href="{{{ cr.links.act_campaign_create }}}?copy={{ lc.act_campaignTab.data.0.rid }}" role="button">{{{ cr.T.btn_Copy }}}</a>
  <a class="btn btn-secondary" href="{{{ cr.links.act_campaign_create }}}" role="button">{{{ cr.T.btn_New }}}</a>
  <a class="btn btn-secondary" href="{{{ cr.links.act_campaign_list }}}/1" role="button">{{{ cr.T.btn_List }}}</a>
</div>

</div><!-- /container -->
</main>

{{> footer }}
{{> close_html }}